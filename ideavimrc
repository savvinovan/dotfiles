set surround
set vim-multiple-cursors
set clipboard=unnamedplus,unnamed
set relativenumber
set number
set visualbell
set noerrorbells
set NERDTree

let mapleader = " "

nmap <leader>s :action KJumpAction<cr>
nmap <leader>w :action KJumpAction.Word0<cr>
nmap <leader>l :action KJumpAction.Line<cr>
nmap gi :action GotoImplementation<cr>
nmap <leader>goimp :action GoImportsFileAction<cr>
nmap <leader>def :action RestoreDefaultLayout<cr>
nmap <leader>movo :action MoveEditorToOppositeTabGroup<cr>
nmap <leader>fmt :action GoFmtFileAction<cr>
nmap <leader>reopen :action ReopenClosedTab<cr>
nmap <leader>be :action Back<cr>
nmap <leader>n :action NextProjectWindow<cr>
nmap <leader>rr :action Rerun<cr>
nmap <leader>ru :action Run<cr>
nmap <leader>rc :action RunCoverage<cr>
nmap <leader>ttp :action TestTreePopupMenu<cr>
nmap <leader>tta :action HTTPClient.RunAll<cr>
nmap <C-h> :action NextSplitter<CR>
nmap <leader>l :action NextSplitter<CR>
nmap <C-l> :action NextSplitter<CR>
nmap <leader>j :action PreviousTab<cr>
nmap <leader>dis :action ToggleDistractionFreeMode<cr>
nmap <leader>ren :action RenameElement<cr>
nmap <C-n> :NERDTreeToggle<cr>
map <F10> :action de.netnexus.CamelCasePlugin.ToggleCamelCase<cr>
nmap <leader>n :NERDTreeToggle<cr>
nmap <M-g> :action Switcher<cr>
nnoremap g; :action JumpToLastChange<CR>
nnoremap g, :action JumpToNextChange<CR>
nmap <leader>dn :action NextOccurence<cr>
map <leader>dr :action Debugger.ResumeThread<cr>
